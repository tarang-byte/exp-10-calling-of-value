#include <iostream>
using namespace std;
void increment(int &s)
{
    s= s+5000;
    cout<<"salary inside function"<<s<<endl;
   
}
int main()
{
    int sal=27000;
    increment(sal);
    
     cout<<"salary inside main"<<sal<<endl;
    return 0;
    
}

output:
salary inside function32000
salary inside main32000
